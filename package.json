{
  "name": "@modernpoacher/isomorphic",
  "version": "0.0.12",
  "description": "Isomorphic React + Redux + React Router",
  "main": "./index.js",
  "author": {
    "email": "modernpoacher@modernpoacher.com",
    "name": "Jonathan Perry for Modern Poacher Limited",
    "url": "http://www.modernpoacher.com"
  },
  "repository": {
    "type": "git",
