{
  "name": "@modernpoacher/isomorphic",
  "version": "0.0.250",
  "description": "Isomorphic React + Redux + React Router",
  "main": "./index.js",
  "author": {
    "email": "modernpoacher@modernpoacher.com",
    "name": "Jonathan Perry for Modern Poacher Limited",
    "url": "http://www.modernpoacher.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/modernpoacher/isomorphic.git"
  },
  "scripts": {
    "prepublishOnly": "cross-env NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env NODE_ENV=test mocha test --recursive -r @babel/register",
    "gulp": "gulp --color",
    "pre-commit": "npm run lint && npm test && npm run build && git add lib",
    "post-commit": "cross-env DEBUG=@modernpoacher/hooks,@modernpoacher/hooks:* npm run gulp -- post-commit"
  },
  "dependencies": {
    "@sequencemedia/react-redux-render": "1.0.14",
    "@sequencemedia/react-render": "1.0.14",
    "@sequencemedia/react-router-redux-render": "1.0.17",
    "@sequencemedia/react-router-render": "1.0.17",
    "debug": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/register": "^7.18.9",
    "@modernpoacher/hooks": "^1.0.155",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.6",
    "core-js": "^3.24.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "gulp": "^4.0.2",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "^8.0.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "^4.2.0",
    "rimraf": "^3.0.2"
  }
}
