    "husky": "^7.0.2",
    "mocha": "^9.1.1",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "^7.2.5",
    "react-router": "5.2.0",
    "react-router-dom": "5.3.0",
    "redux": "^4.1.1",
    "rimraf": "^3.0.2"
