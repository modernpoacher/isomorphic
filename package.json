    "debug": "^4.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/preset-env": "^7.15.6",
    "@babel/register": "^7.15.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.4",
    "core-js": "^3.18.0",
